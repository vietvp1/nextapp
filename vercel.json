{
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=1"
                },
                {
                    "key": "CDN-Cache-Control",
                    "value": "public, s-maxage=30"
                },
                {
                    "key": "Vercel-CDN-Cache-Control",
                    "value": "public, s-maxage=30"
                }
            ]
        }
    ],
    "functions": {
        "app/api/**/*": {
            "maxDuration": 5
        },
        "pages/api/**/*": {
            "maxDuration": 5
        }
    }
}