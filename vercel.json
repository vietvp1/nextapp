{
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=20"
                }
            ]
        }
    ],
    "functions": {
        "app/api/**/*": {
            "maxDuration": 5
        },
        "pages/api/**/*": {
            "maxDuration": 5
        }
    }
}